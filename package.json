    "prebuild": "node ./script/pre-build.js",
    "build": "yarn prebuild && next build && yarn postbuild",
    "postbuild": "node ./script/post-build.js",
    "build:vet": "export MODE_PROFILE=vet && turbo run build",
    "build:tutor": "export MODE_PROFILE=tutor && turbo run build",
    "build:external": "export MODE_PROFILE=external && turbo run build",
    "turbo": "turbo run build",
    "start": "next start -p 3001",
    "lint": "npx @biomejs/biome check --apply-unsafe ./source",
    "format": "npx @biomejs/biome format --write ./source",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "vitest",
    "test:browser": "vitest --workspace=vitest.workspace.ts"
